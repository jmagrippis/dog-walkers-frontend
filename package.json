{
  "name": "dog-walkers",
  "version": "0.0.1",
  "description": "The frontend of an app that finds available dog walkers in your area.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "keywords": [
    "react",
    "redux",
    "dogs",
    "walkers",
    "london"
  ],
  "author": "Johnny Magrippis <j@magrippis.com>",
  "license": "MIT",
  "dependencies": {
    "aphrodite": "^0.2.0",
    "immutable": "^3.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-0": "^6.0.0",
    "chai": "^3.0.0",
    "chai-immutable": "^1.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.0",
    "jsdom": "^8.0.0",
    "mocha": "^2.0.0",
    "normalize.css": "^4.0.0",
    "react-hot-loader": "^3.0.0-0",
    "style-loader": "^0.13.0",
    "webpack": "^1.0.0",
    "webpack-dev-server": "^1.0.0"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/jmagrippis/dog-walkers-frontend"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  }
}
